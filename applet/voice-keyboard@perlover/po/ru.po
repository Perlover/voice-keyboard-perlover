msgstr "Сервер не ответил вовремя"

#. applet.js - notification (custom prompts)
msgid "AI chat processing failed"
msgstr "Ошибка обработки AI-чатом"

#. settings-schema.json
msgid "Whisper mode"
msgstr "Режим Whisper"

#. settings-schema.json
msgid "OpenAI API"
msgstr "OpenAI API"

#. settings-schema.json
msgid "Local Server"
msgstr "Локальный сервер"

#. settings-schema.json
msgid "Choose between OpenAI API or local Whisper server"
msgstr "Выберите OpenAI API или локальный сервер Whisper"

#. settings-schema.json
msgid "OpenAI API Key"
msgstr "Ключ API OpenAI"

#. settings-schema.json
msgid "Your OpenAI API key for Whisper (required if using OpenAI mode)"
msgstr "Ваш ключ API OpenAI для Whisper (обязателен в режиме OpenAI)"

#. settings-schema.json
msgid "OpenAI Model"
msgstr "Модель OpenAI"

#. settings-schema.json
msgid "Whisper-1 (standard, $0.006/min)"
msgstr "Whisper-1 (стандартная, $0.006/мин)"

#. settings-schema.json
msgid "GPT-4o Transcribe (best quality, $0.006/min)"
msgstr "GPT-4o Transcribe (лучшее качество, $0.006/мин)"

#. settings-schema.json
msgid "Whisper-1: Standard speech-to-text. GPT-4o Transcribe: Latest model with best accuracy for all languages"
msgstr "Whisper-1: Стандартное распознавание речи. GPT-4o Transcribe: Новейшая модель с лучшей точностью для всех языков"

#. settings-schema.json
msgid "Local Whisper Server URL"
msgstr "URL локального сервера Whisper"

#. settings-schema.json
msgid "URL of your local Whisper server (e.g., faster-whisper-server)"
msgstr "URL вашего локального сервера Whisper (напр., faster-whisper-server)"

#. settings-schema.json
msgid "Recognition language"
msgstr "Язык распознавания"

#. settings-schema.json
msgid "Auto-detect"
msgstr "Автоопределение"

#. settings-schema.json
msgid "Russian"
msgstr "Русский"

#. settings-schema.json
msgid "English"
msgstr "Английский"

#. settings-schema.json
msgid "Spanish"
msgstr "Испанский"

#. settings-schema.json
msgid "German"
msgstr "Немецкий"

#. settings-schema.json
msgid "French"
