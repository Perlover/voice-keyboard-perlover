msgid "AI chat processing failed"
msgstr "AIチャット処理に失敗しました"

#. settings-schema.json
msgid "Whisper mode"
msgstr "Whisper モード"

#. settings-schema.json
msgid "OpenAI API"
msgstr "OpenAI API"

#. settings-schema.json
msgid "Local Server"
msgstr "ローカルサーバー"

#. settings-schema.json
msgid "Choose between OpenAI API or local Whisper server"
msgstr "OpenAI API またはローカル Whisper サーバーを選択"

#. settings-schema.json
msgid "OpenAI API Key"
msgstr "OpenAI APIキー"

#. settings-schema.json
msgid "Your OpenAI API key for Whisper (required if using OpenAI mode)"
msgstr "Whisper 用の OpenAI APIキー（OpenAI モード使用時に必要）"

#. settings-schema.json
msgid "OpenAI Model"
msgstr "OpenAI モデル"

#. settings-schema.json
msgid "Whisper-1 (standard, $0.006/min)"
msgstr "Whisper-1（標準、$0.006/分）"

#. settings-schema.json
msgid "GPT-4o Transcribe (best quality, $0.006/min)"
msgstr "GPT-4o Transcribe（最高品質、$0.006/分）"

#. settings-schema.json
msgid "Whisper-1: Standard speech-to-text. GPT-4o Transcribe: Latest model with best accuracy for all languages"
msgstr "Whisper-1：標準の音声認識。GPT-4o Transcribe：全言語で最高の精度を持つ最新モデル"

#. settings-schema.json
msgid "Local Whisper Server URL"
msgstr "ローカル Whisper サーバー URL"

#. settings-schema.json
msgid "URL of your local Whisper server (e.g., faster-whisper-server)"
msgstr "ローカル Whisper サーバーの URL（例：faster-whisper-server）"

#. settings-schema.json
msgid "Recognition language"
msgstr "認識言語"

#. settings-schema.json
msgid "Auto-detect"
msgstr "自動検出"

#. settings-schema.json
msgid "Russian"
msgstr "ロシア語"

#. settings-schema.json
msgid "English"
msgstr "英語"

#. settings-schema.json
