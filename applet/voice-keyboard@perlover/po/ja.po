#. settings-schema.json
msgid "French"
msgstr "フランス語"

#. settings-schema.json
msgid "Chinese"
msgstr "中国語"

#. settings-schema.json
msgid "Japanese"
msgstr "日本語"

#. settings-schema.json
msgid "Language for speech recognition"
msgstr "音声認識の言語"

#. settings-schema.json
msgid "Maximum Recording Duration"
msgstr "最大録音時間"

#. settings-schema.json
msgid "Safety limit for maximum recording duration. Recording will automatically stop and transcribe when this limit is reached. Default: 5 minutes (300 seconds)."
msgstr "最大録音時間の安全制限。この制限に達すると、録音は自動的に停止して文字起こしされます。デフォルト：5分（300秒）。"

#. settings-schema.json
msgid "Path to whisper-voice-input script"
msgstr "whisper-voice-input スクリプトのパス"

#. settings-schema.json
msgid "Full path to the voice input script"
msgstr "音声入力スクリプトのフルパス"

#. settings-schema.json
msgid "Chat AI Model"
msgstr "Chat AI モデル"

#. settings-schema.json
msgid "GPT-4o Mini (fast, economical)"
msgstr "GPT-4o Mini（高速、経済的）"

#. settings-schema.json
msgid "GPT-4o (best quality)"
msgstr "GPT-4o（最高品質）"

#. settings-schema.json
msgid "GPT-5 (latest)"
msgstr "GPT-5（最新）"

#. settings-schema.json
msgid "Model for processing text with custom prompts"
msgstr "カスタムプロンプトでテキストを処理するモデル"

#. settings-schema.json - custom-prompts-help label
msgid "Instructions: Click '+' to add. Use AI to process text. Example: Name='Fix Grammar', Prompt='Correct all typos in the following text:'"
msgstr "説明：「+」をクリックして追加。AIがテキストを処理します。例：名前=「文法修正」、プロンプト=「次のテキストの誤字をすべて修正してください：」"

#. settings-schema.json
msgid "Custom Prompts: click '+' to add. Use AI to process recognized text."
msgstr "プロンプト：「+」をクリックして追加。AIが認識テキストを処理します。"

#. settings-schema.json
msgid "Enabled: toggle prompt. Menu Name: how it appears in right-click menu. System Prompt: instructions for the AI (e.g., 'Translate to French' or 'Fix grammar')."
msgstr "有効：プロンプトの切替。メニュー名：右クリックメニューでの表示名。システムプロンプト：AIへの指示（例：「フランス語に翻訳」や「文法を修正」）。"

#. settings-schema.json - custom-prompts column title
msgid "Enabled"
msgstr "有効"

#. settings-schema.json - custom-prompts column title
msgid "Menu Name (e.g., 'My Prompt')"
msgstr "メニュー名（例：「マイプロンプト」）"

#. settings-schema.json - custom-prompts column title
msgid "System Prompt (AI instructions)"
msgstr "システムプロンプト（AI指示）"

#. settings-schema.json - export/import header
msgid "Export / Import Settings"
msgstr "設定のエクスポート / インポート"

#. settings-schema.json - export button
msgid "Export Settings to File..."
msgstr "設定をファイルにエクスポート..."

#. settings-schema.json - export button tooltip
msgid "Save all settings to a JSON file"
msgstr "すべての設定を JSON ファイルに保存"

#. settings-schema.json - export button tooltip
msgid "Copy all settings to clipboard as JSON"
msgstr "すべての設定を JSON としてクリップボードにコピー"

#. settings-schema.json - import button
msgid "Import Settings from File..."
msgstr "ファイルから設定をインポート..."

#. settings-schema.json - import button tooltip
msgid "Load settings from a JSON file"
msgstr "JSON ファイルから設定を読み込む"

#. settings-schema.json - import button tooltip
msgid "Paste settings from clipboard JSON"
msgstr "クリップボードの JSON から設定を貼り付け"

#. applet.js - export/import menu
msgid "Export Settings to Clipboard"
msgstr "設定をクリップボードにエクスポート"

#. applet.js - export/import menu
msgid "Import Settings from Clipboard"
msgstr "クリップボードから設定をインポート"

#. applet.js - import dialog title
msgid "Import Settings"
msgstr "設定のインポート"

#. applet.js - import dialog info
msgid "Version: %s"
msgstr "バージョン：%s"

#. applet.js - import dialog info
msgid "Date: %s"
msgstr "日付：%s"

#. applet.js - import dialog info
msgid "Settings count: %d"
msgstr "設定数：%d"

#. applet.js - import dialog warning
msgid "All current settings will be overwritten. Continue?"
msgstr "すべての現在の設定が上書きされます。続行しますか？"

#. applet.js - import dialog button
msgid "Cancel"
msgstr "キャンセル"

#. applet.js - import dialog button
msgid "Import"
msgstr "インポート"

#. applet.js - export notification
msgid "Warning: exported data contains your API key. Keep it secure!"
msgstr "警告：エクスポートされたデータにはAPIキーが含まれています。安全に保管してください！"

#. applet.js - export notification
msgid "Failed to export settings"
msgstr "設定のエクスポートに失敗しました"

#. applet.js - export notification
msgid "Settings exported to clipboard"
msgstr "設定をクリップボードにエクスポートしました"

