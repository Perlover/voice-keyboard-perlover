msgid "Chinese"
msgstr "Chinesisch"

#. settings-schema.json
msgid "Japanese"
msgstr "Japanisch"

#. settings-schema.json
msgid "Language for speech recognition"
msgstr "Sprache für Spracherkennung"

#. settings-schema.json
msgid "Maximum Recording Duration"
msgstr "Maximale Aufnahmedauer"

#. settings-schema.json
msgid "Safety limit for maximum recording duration. Recording will automatically stop and transcribe when this limit is reached. Default: 5 minutes (300 seconds)."
msgstr "Sicherheitslimit für die maximale Aufnahmedauer. Die Aufnahme wird automatisch gestoppt und transkribiert, wenn dieses Limit erreicht wird. Standard: 5 Minuten (300 Sekunden)."

#. settings-schema.json
msgid "Path to whisper-voice-input script"
msgstr "Pfad zum whisper-voice-input-Skript"

#. settings-schema.json
msgid "Full path to the voice input script"
msgstr "Vollständiger Pfad zum Spracheingabe-Skript"

#. settings-schema.json
msgid "Chat AI Model"
msgstr "Chat-AI-Modell"

#. settings-schema.json
msgid "GPT-4o Mini (fast, economical)"
msgstr "GPT-4o Mini (schnell, wirtschaftlich)"

#. settings-schema.json
msgid "GPT-4o (best quality)"
msgstr "GPT-4o (beste Qualität)"

#. settings-schema.json
msgid "GPT-5 (latest)"
msgstr "GPT-5 (neueste)"

#. settings-schema.json
msgid "Model for processing text with custom prompts"
msgstr "Modell zur Textverarbeitung mit benutzerdefinierten Prompts"

#. settings-schema.json - custom-prompts-help label
msgid "Instructions: Click '+' to add. Use AI to process text. Example: Name='Fix Grammar', Prompt='Correct all typos in the following text:'"
msgstr "Anleitung: Klicken Sie auf '+' zum Hinzufügen. AI verarbeitet den Text. Beispiel: Name='Grammatik korrigieren', Prompt='Korrigiere alle Tippfehler im folgenden Text:'"

#. settings-schema.json
msgid "Custom Prompts: click '+' to add. Use AI to process recognized text."
msgstr "Prompts: Klicken Sie auf '+' zum Hinzufügen. AI verarbeitet den erkannten Text."

#. settings-schema.json
msgid "Enabled: toggle prompt. Menu Name: how it appears in right-click menu. System Prompt: instructions for the AI (e.g., 'Translate to French' or 'Fix grammar')."
msgstr "Aktiviert: Prompt ein-/ausschalten. Menüname: Anzeige im Kontextmenü. System-Prompt: Anweisungen für die AI (z.B. 'Übersetze ins Französische' oder 'Grammatik korrigieren')."

#. settings-schema.json - custom-prompts column title
msgid "Enabled"
msgstr "Aktiviert"

#. settings-schema.json - custom-prompts column title
msgid "Menu Name (e.g., 'My Prompt')"
msgstr "Menüname (z.B. 'Mein Prompt')"

#. settings-schema.json - custom-prompts column title
msgid "System Prompt (AI instructions)"
msgstr "System-Prompt (AI-Anweisungen)"

